{
  "name": "leo-simulator",
  "version": "1.0.0",
  "description": "LEO Satellite Handover Visualization - NTPU 3D Scene",
  "type": "module",
  "scripts": {
    "predev": "npm run clean:ts-output",
    "dev": "vite",
    "prebuild": "npm run clean:ts-output",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "clean:ts-output": "node ./scripts/clean-ts-output.js"
  },
  "keywords": [
    "satellite",
    "leo",
    "3d",
    "visualization",
    "handover"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.1.0",
    "sass": "^1.93.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}
